# Copy this to .env and fill in your values

# Pinecone API Key (required for vector storage)
PINECONE_API_KEY=your_pinecone_api_key_here

# OpenRouter API Key (for LLM calls)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Specific model to use via OpenRouter
OPENROUTER_MODEL=google/gemini-3-flash-preview
SYNTHESIS_MODEL=google/gemini-3-flash-preview
ROUTER_MODEL=google/gemini-3-flash-preview
OPENAI_GENERATION_MODEL=gpt-4o-mini

# Optional: Embedding model (if using OpenAI embeddings)
OPENAI_API_KEY=your_openai_api_key_here
